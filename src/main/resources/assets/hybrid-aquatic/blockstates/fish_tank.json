{
  "multipart": [
    {
      "when" : {
        "down": "true"
      },
      "apply": {
        "model": "hybrid-aquatic:block/fish_tank/floor"
      }
    },
    {
      "when" : {
        "up": "true"
      },
      "apply": {
        "model": "hybrid-aquatic:block/fish_tank/roof"
      }
    },
    {
      "when" : {
        "north": "true"
      },
      "apply": {
        "model": "hybrid-aquatic:block/fish_tank/wall_north"
      }
    },
    {
      "when" : {
        "south": "true"
      },
      "apply": {
        "model": "hybrid-aquatic:block/fish_tank/wall_south"
      }
    },
    {
      "when" : {
        "east": "true"
      },
      "apply": {
        "model": "hybrid-aquatic:block/fish_tank/wall_east"
      }
    },
    {
      "when" : {
        "west": "true"
      },
      "apply": {
        "model": "hybrid-aquatic:block/fish_tank/wall_west"
      }
    },
    {
      "when" : {
        "north" : "true",
        "west" : "true"
      },
      "apply": {
        "model": "hybrid-aquatic:block/fish_tank/corner_nw"
      }
    },
    {
      "when" : {
        "north" : "true",
        "east" : "true"
      },
      "apply": {
        "model": "hybrid-aquatic:block/fish_tank/corner_ne"
      }
    },
    {
      "when" : {
        "south" : "true",
        "west" : "true"
      },
      "apply": {
        "model": "hybrid-aquatic:block/fish_tank/corner_sw"
      }
    },
    {
      "when" : {
        "south" : "true",
        "east" : "true"
      },
      "apply": {
        "model": "hybrid-aquatic:block/fish_tank/corner_se"
      }
    }
  ]
}